        "#sefe khati\n",
        "#nokte:در صف خطی اگر ریر یه انتهای صف یرسد یاید تمام صف خالی شود تا دوباره بتوان از این صف استفاده کرد\n",
        "class Queue:\n",
        "    def __init__(self,k):\n",
        "        self.k=k\n",
        "        self.queue=[None]*k\n",
        "        self.front=-1\n",
        "        self.rear=-1\n",
        "    def disqueue(self):\n",
        "        if self.front == -1:\n",
        "            print(\"empty\")\n",
        "        for i in range(self.front,self.rear+1):\n",
        "            print(self.queue[i])\n",
        "    def insertqueue(self,data):\n",
        "        if(self.front==-1):\n",
        "            self.front=0\n",
        "            self.rear=0\n",
        "            self.queue[self.rear]=data\n",
        "        elif(self.rear+1==self.k):\n",
        "            print(\"is Full\")\n",
        "        else:\n",
        "            self.rear+=1\n",
        "            self.queue[self.rear]=data\n",
        "        def deletequeue(self):\n",
        "            if (self.front==-1):\n",
        "                print(\"empty\")\n",
        "            elif (self.front==self.rear):\n",
        "                t=self.queue[self.front]\n",
        "                self.front=-1\n",
        "                self.rear=-1\n",
        "                return t\n",
        "            else:\n",
        "                t=self.queue[self.front]\n",
        "                self.front+=1\n",
        "                return t\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        ""
